.member_news_feed_item
  =link_to(image_for(member, "60x"), member)
  .news_feed_item_content
    .inner_text
      -if news_feed_item.updated?
        ="#{link_to(member, member)} updated their profile."
      -else
        ="#{link_to(member, member)} joined #{site_name}."
      .time_ago
        ="#{time_ago_in_words(news_feed_item.created_at)} ago"
      .news_feed_option_links  
        =news_feed_option_links(news_feed_item)
  %br.clear